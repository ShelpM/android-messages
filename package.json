{
  "name": "androidmessages",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . android-messages --overwrite --platform=darwin --arch=x64 --icon=assets/icons/darwin/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . android-messages --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win32/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Android Messages\"",
    "package-linux": "electron-packager . android-messages --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "author": "Shelp",
  "license": "Unlicense",
  "dependencies": {
    "electron": "^4.0.2"
  },
  "devDependencies": {
    "electron-packager": "^13.0.1"
  }
}
